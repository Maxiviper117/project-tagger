{
  "name": "@maxiviper117/project-tagger",
  "version": "2024.12.07",
  "description": "A powerful CLI tool for managing tags in projects.",
  "main": "dist/tagmaster.js",
  "module": "src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maxiviper117/tagmaster.git"
  },
  "author": "Maxiviper117",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "cli",
    "tags",
    "project-management",
    "typescript"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --compile --outfile ./dist/tagmaster",
    "build:windows": "bun build ./src/index.ts --compile --target=bun-windows-x64 --outfile ./dist/tagmaster.exe",
    "build:linux": "bun build ./src/index.ts --compile --target=bun-linux-x64 --outfile ./dist/tagmaster",
    "build:mac": "bun build ./src/index.ts --compile --target=bun-darwin-x64 --outfile ./dist/tagmaster",
    "test": "bun test",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "start": "bun run ./dist/tagmaster"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.10.1",
    "eslint": "^8.56.1",
    "prettier": "^3.1.8",
    "ts-node": "^11.1.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^12.1.0"
  }
}
